# Mossy Backend (example)
# Copy to `.env` on your server (Render/VM/etc).

PORT=8787

# REQUIRED for hosted deployments (turns on auth)
MOSSY_API_TOKEN=change-me-to-a-long-random-string

# Provider keys (kept on server only)
GROQ_API_KEY=
OPENAI_API_KEY=

# Optional default models
GROQ_MODEL=llama-3.1-70b-versatile
OPENAI_MODEL=gpt-4o-mini
OPENAI_TRANSCRIBE_MODEL=whisper-1

# Basic rate limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=60
